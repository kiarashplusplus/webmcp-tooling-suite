{
  "name": "@25xcodes/llmfeed-health-monitor",
  "version": "1.1.3",
  "description": "Feed health monitoring, crawling, and outreach automation for the LLMFeed ecosystem",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "llmfeed-health": "./bin/llmfeed-health.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "llmfeed",
    "webmcp",
    "mcp",
    "health-monitor",
    "feed-crawler",
    "validation",
    "ed25519",
    "bot",
    "outreach"
  ],
  "author": "25xcodes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiarashplusplus/webmcp-tooling-suite.git",
    "directory": "packages/health-monitor"
  },
  "devDependencies": {
    "@25xcodes/llmfeed-validator": "^1.0.0",
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "^4.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^4.0.0"
  },
  "peerDependencies": {
    "@25xcodes/llmfeed-validator": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@25xcodes/llmfeed-validator": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "better-sqlite3": "^11.0.0",
    "nodemailer": "^6.9.0",
    "node-cron": "^3.0.0",
    "cron-parser": "^4.9.0"
  }
}
